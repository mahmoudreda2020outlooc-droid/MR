{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Security-Test",
          "value": "passed"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self';"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/dashboard.html"
    }
  ]
}